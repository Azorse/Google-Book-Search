{
  "name": "google-book-search",
  "version": "1.0.0",
  "description": "React Book Search",
  "main": "server.js",
  "engines": {
    "node": "12.13.1"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Azorse/Google-Book-Search.git"
  },
  "keywords": [
    "Google",
    "React",
    "Book"
  ],
  "author": "Dan Schissler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Azorse/Google-Book-Search/issues"
  },
  "homepage": "https://github.com/Azorse/Google-Book-Search#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "mongoose": "^5.9.6",
    "react-router-dom": "^5.1.2",
    "concurrently": "^5.1.0",
    "if-env": "^1.0.4",
    "nodemon": "^2.0.2"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "if-env": "^1.0.4",
    "nodemon": "^2.0.2"
  }
}
